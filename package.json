{
  "name": "@kinde/js-utils",
  "private": false,
  "description": "Selection of JavaScript utilities for integrating with Kinde",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kinde-oss/js-utils.git"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "version": "0.14.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "prettier --check . && eslint .",
    "lint:fix": "prettier --write . && eslint . --fix"
  },
  "module": "dist/js-utils.js",
  "main": "dist/js-utils.cjs",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/js-utils.js",
      "require": "./dist/js-utils.cjs",
      "default": "./dist/js-utils.js"
    }
  },
  "sideEffects": false,
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/chrome": "^0.0.317",
    "@types/node": "^22.5.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.0.0",
    "eslint": "^9.9.1",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.0.3",
    "vitest": "^3.0.0",
    "vitest-fetch-mock": "^0.4.1"
  },
  "peerDependencies": {
    "expo-secure-store": ">=11.0.0"
  },
  "peerDependenciesMeta": {
    "expo-secure-store": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
  "dependencies": {
    "@kinde/jwt-decoder": "^0.2.0"
  }
}
